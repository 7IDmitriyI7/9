using System;

namespace _9._1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите кол-во секунд");
            int a = Convert.ToInt32(Console.ReadLine());
            int b = 0;
            if (a >= 0)
            {
                b = a % 60;
                Console.WriteLine("Кол-во сек прошедших с последней мин " + b);
            }
            else
            {
                Console.WriteLine("Ошибка ввода");
            }

        }
    }
}
